<h1>New permission</h1>

<% form_for(@permission) do |f| %>
  <%= f.error_messages %>
  <%= f.hidden_field(:repository_id) %>
  <%= f.hidden_field(:user_id) %>
  User: <input id="user" type="text" name="user">
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>
<%= link_to 'Back', permissions_path %>
