<%= page_heading "New sshkey" %>
<h1>New SSH Key</h1>

<% form_for(@sshkey) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :key %><br />
    <%= f.text_area :key %>
  </p>
  <p>
    <%= f.submit "Add SSH key" %>
  </p>
<% end %>
