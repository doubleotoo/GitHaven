<%= page_heading "Edit sshkey" %>
<h1>Edit sshkey</h1>

<% form_for(@sshkey) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :key %><br />
    <%= f.text_area :key %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>
